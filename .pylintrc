[REPORTS]
output-format=parseable
reports=no

[MESSAGES CONTROL]
disable=too-many-locals,too-many-arguments,too-many-branches,too-many-return-statements,too-many-statements,too-many-lines,line-too-long,logging-fstring-interpolation,missing-function-docstring,missing-module-docstring,missing-class-docstring,broad-except,too-many-instance-attributes

[LOGGING]
logging-format-style=new

[FORMAT]
max-line-length=200

[MASTER]
extension-pkg-whitelist=pydantic
